        body {
            margin: 0;
            overflow-x: hidden;
            display: flex;
            flex-direction: column;
            font-family: 'Mukta', sans-serif;
            font-size: 2vh;
            background-color: #fff8f8;
        }

        .nav {
            position: fixed;
            top: 20px;
            left: 20px;
            z-index: 1000;
            font-family: 'Mukta', sans-serif;
            font-size: 2vh;
        }

        header {
            background-color: rgb(25, 129, 48);
            height: 100vh;
            display: flex;
            align-items: center;
            justify-content: center;
            text-align: center;
            overflow: hidden;
            flex-direction: column;
            z-index: 1;
        }

        h1, h2 {
            color: #ffffff;
            margin: 2vh;
            font-family: 'Anton', sans-serif;
            white-space: nowrap;
            overflow: hidden;
            z-index: 1;
        }

        h1 {
            font-size: 26vh;
            animation: fall-h1 3s forwards;
        }

        h2 {
            font-size: calc(26vh * 1.2);
            animation: fall-h2 3s forwards;
        }

        @keyframes fall-h1 {
            0% {
                transform: translateY(-100%);
            }

            100% {
                transform: translateY(0);
            }
        }

        @keyframes fall-h2 {
            0% {
                transform: translateY(100%);
            }

            100% {
                transform: translateY(0);
            }
        }

        #presidente {
            display: block;
            margin: 2vh;
            align-self: center;
            max-height: 110%;
            max-width: 110%;
            z-index: 1;
            cursor: pointer;
        }

        .nav, .menu {
            display: flex;
            justify-content: center;
            align-items: center;
            padding: auto;
            margin-bottom: 5px;
        }

        .nav {
            background-color: #fff;
            padding: 20px;
            transition: 0.5s;
            border-radius: 50px;
            overflow: hidden;
            box-shadow: 0 8px 15px rgba(0, 0, 0, .2);
            z-index: 2;
        }

        .menu {
            margin: 0;
            padding: 0;
            width: 0;
            overflow: hidden;
            transition: 0.5s;
        }

        .nav input:checked~.menu {
            width: 450px;
        }

        .menu li {
            list-style: none;
            margin: 0 10px;
        }

        .menu li a {
            text-decoration: none;
            color: #666;
            text-transform: uppercase;
            font-weight: 600;
            transition: 0.5s;
        }

        .menu li a:hover {
            color: #161919;
        }

        .nav input {
            width: 40px;
            height: 40px;
            cursor: pointer;
            opacity: 0;
            z-index: 2;
        }

        .nav span {
            position: absolute;
            left: 30px;
            width: 30px;
            height: 4px;
            border-radius: 50px;
            background-color: #666;
            pointer-events: none;
            transition: 0.5s;
            z-index: 2;
        }

        .nav input:checked~span {
            background-color: #f974a1;
        }

        .nav span:nth-child(2) {
            transform: translateY(-8px);
        }

        .nav input:checked~span:nth-child(2) {
            transform: translateY(0) rotate(-45deg);
        }

        .nav span:nth-child(3) {
            transform: translateY(8px);
        }

        .nav input:checked~span:nth-child(3) {
            transform: translateY(0) rotate(45deg);
        }

        .click {
            float: right;
            position: absolute;
            padding-top: 10ch;
            padding-left: 5ch;
